{
    "version": "1.0.0",
    "tasks": [
      {
        "label": "Build Gecko Codes, run VSD Sync",
        "type": "shell",
        "command": "powershell",
        "args": [
          "-ExecutionPolicy",
          "Bypass",
          "-File",
          "build.ps1"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": []
      },
      {
        "label": "Build Release SD Card with VSD Sync",
        "type": "shell",
        "command": "powershell",
        "args": [
          "-ExecutionPolicy",
          "Bypass",
          "-File",
          "make_release.ps1"
        ],
        "group": {
          "kind": "build",
          "isDefault": false
        }
      }
    ]
  }